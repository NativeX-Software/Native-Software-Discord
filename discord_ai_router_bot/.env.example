# Copy to .env and fill in the provider credentials before running the bot
DISCORD_BOT_TOKEN=
# Optional: scope slash command registration to a single guild ID
DISCORD_GUILD_ID=
# Optional defaults for /ai
DEFAULT_PROVIDER=openai
DEFAULT_MODEL=gpt-4o-mini
# Optional rate limits (requests per window / window seconds)
AI_RATE_LIMIT=5
AI_RATE_WINDOW=60
# Set to 1 to enable message content intent (requires privileged intent in Discord portal)
ENABLE_MESSAGE_CONTENT=0
# Provider API keys (set the ones you plan to use)
OPENAI_API_KEY=
OPENAI_BASE_URL=
ANTHROPIC_API_KEY=
ANTHROPIC_VERSION=2023-06-01
GEMINI_API_KEY=
GEMINI_BASE_URL=
GROK_API_KEY=
GROK_BASE_URL=https://api.x.ai
# Optional logging level
LOG_LEVEL=INFO
